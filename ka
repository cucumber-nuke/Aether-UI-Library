    local Players = game:GetService("Players")
    local Player = Players.LocalPlayer
    
    -- 머더인지 확인
    if not Player.Character:FindFirstChild("Knife") then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "오류",
            Text = "머더가 아닙니다!",
            Duration = 3
        })
        return
    end
    
    -- 플링 설정
    getgenv().FPDH = workspace.FallenPartsDestroyHeight
    
    -- 모든 플레이어에 대해 플링 실행
    for _, target in pairs(Players:GetPlayers()) do
        if target ~= Player then
            local Character = Player.Character
            local Humanoid = Character and Character:FindFirstChildOfClass("Humanoid")
            local RootPart = Humanoid and Humanoid.RootPart
            
            local TCharacter = target.Character
            if TCharacter then
                local THumanoid = TCharacter:FindFirstChildOfClass("Humanoid")
                local TRootPart = THumanoid and THumanoid.RootPart
                
                if TRootPart then
                    -- 플링 실행
                    local BV = Instance.new("BodyVelocity")
                    BV.Name = "KillVelocity"
                    BV.Parent = RootPart
                    BV.Velocity = Vector3.new(9999999, 9999999, 9999999)
                    BV.MaxForce = Vector3.new(1/0, 1/0, 1/0)
                    
                    local oldPos = RootPart.CFrame
                    
                    -- 타겟에게 이동 및 플링
                    RootPart.CFrame = TRootPart.CFrame
                    wait(0.1)
                    BV:Destroy()
                    
                    -- 원래 위치로 복귀
                    RootPart.CFrame = oldPos
                end
            end
            wait(0.5) -- 다음 플레이어로 넘어가기 전 대기
        end
    end
